{
  "name": "maksym/wget-lib",
  "type": "library",
  "description": "Wget-package for small framework",
  "keywords": [
    "wget",
    "lib",
    "curl"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Maksym Haivoronskyi",
      "email": "maksym.haivoronskyi@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.3.0",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-curl": "*",
    "maksym/config-lib": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "@stable"
  },
  "autoload": {
    "files": [
      "src/helper.php"
    ],
    "psr-4": {
      "Maksym\\Wget\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
